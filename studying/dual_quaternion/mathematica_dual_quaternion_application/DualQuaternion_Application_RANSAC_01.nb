    RowBox[{"j", "\[LessEqual]", "\[ScriptCapitalN]"}], ",", 
    RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"get", " ", "four", " ", "random", " ", "points"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"dataS", "=", 
      RowBox[{"Transpose", "[", 
