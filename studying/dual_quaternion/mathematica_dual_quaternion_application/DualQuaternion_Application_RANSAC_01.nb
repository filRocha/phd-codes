Cell[TextData[{
 "For further application, to integrate the dual quaternion solution as a \
core algorithm into the RANSAC method, let us summarize the steps of the \
procedure in a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " function."
}], "Text",
 CellChangeTimes->{{3.6273798140177526`*^9, 3.627379930596757*^9}, {
  3.6280701073809566`*^9, 
  3.6280701421810055`*^9}},ExpressionUUID->"efc22d95-2930-43da-970d-\
5efe0d0aeb0c"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DualQHelmert", "[", 
    RowBox[{"XYZin_", ",", "xyzin_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "XYZ", ",", "xyz", ",", "XYZavg", ",", "xyzavg", ",", "s", ",", "sol", 
       ",", "solq", ",", "tt", ",", "RN", ",", "i", ",", "n", ",", "sel", ",",
        "imin"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"scaling", " ", "factor", " ", "s"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"XYZ", "=", 
       RowBox[{"SetPrecision", "[", 
        RowBox[{"XYZin", ",", "20"}], "]"}]}], ";", 
      RowBox[{"xyz", "=", 
       RowBox[{"SetPrecision", "[", 
        RowBox[{"xyzin", ",", "20"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"XYZavg", "=", 
       RowBox[{"Mean", "[", "XYZ", "]"}]}], ";", 
      RowBox[{"xyzavg", "=", 
       RowBox[{"Mean", "[", "xyz", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"s", "=", 
       RowBox[{"Mean", "[", 
        RowBox[{"MapThread", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"Norm", "[", 
             RowBox[{"#1", "-", "xyzavg"}], "]"}], "/", 
            RowBox[{"Norm", "[", 
             RowBox[{"#2", "-", "XYZavg"}], "]"}]}], "&"}], ",", 
          RowBox[{"{", 
           RowBox[{"xyz", ",", "XYZ"}], "}"}]}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"egyenletek", " ", "quaternion", " ", "elemeire"}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"eqPt1", "=", 
       RowBox[{"pQd1", "-", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", "p2", " ", "q1", " ", "q2"}], "-", 
          RowBox[{"2", " ", "p2", " ", "q0", " ", "q3"}], "+", 
          RowBox[{"2", " ", "p3", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"q0", " ", "q2"}], "+", 
             RowBox[{"q1", " ", "q3"}]}], ")"}]}], "-", 
          RowBox[{"p1", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", 
              SuperscriptBox["q0", "2"]}], "-", 
             SuperscriptBox["q1", "2"], "+", 
             SuperscriptBox["q2", "2"], "+", 
             SuperscriptBox["q3", "2"]}], ")"}]}], "+", " ", "t1"}], 
         ")"}]}]}], ";", "\n", "     ", 
      RowBox[{"eqPt2", "=", 
       RowBox[{"pQd2", "-", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"-", "p3"}], " ", "q0", " ", "q1"}], "+", 
             RowBox[{"p1", " ", "q1", " ", "q2"}], "+", 
             RowBox[{"p1", " ", "q0", " ", "q3"}], "+", 
             RowBox[{"p3", " ", "q2", " ", "q3"}]}], ")"}]}], "-", 
          RowBox[{"p2", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", 
              SuperscriptBox["q0", "2"]}], "+", 
             SuperscriptBox["q1", "2"], "-", 
             SuperscriptBox["q2", "2"], "+", 
             SuperscriptBox["q3", "2"]}], ")"}]}], "+", " ", "t2"}], 
         ")"}]}]}], ";", "\n", "      ", 
      RowBox[{"eqPt3", "=", 
       RowBox[{"pQd3", "-", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"p2", " ", "q0", " ", "q1"}], "-", 
             RowBox[{"p1", " ", "q0", " ", "q2"}], "+", 
             RowBox[{"p1", " ", "q1", " ", "q3"}], "+", 
             RowBox[{"p2", " ", "q2", " ", "q3"}]}], ")"}]}], "-", 
          RowBox[{"p3", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", 
              SuperscriptBox["q0", "2"]}], "+", 
             SuperscriptBox["q1", "2"], "+", 
             SuperscriptBox["q2", "2"], "-", 
             SuperscriptBox["q3", "2"]}], ")"}]}], "+", " ", "t3"}], 
         ")"}]}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"data1", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"p1", "\[Rule]", 
          RowBox[{
           RowBox[{"XYZ", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ",", 
         RowBox[{"p2", "\[Rule]", 
          RowBox[{
           RowBox[{"XYZ", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], ",", 
         RowBox[{"p3", "\[Rule]", 
          RowBox[{
           RowBox[{"XYZ", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "[", 
           RowBox[{"[", "3", "]"}], "]"}]}], ",", 
         RowBox[{"pQd1", "\[Rule]", 
          RowBox[{
           FractionBox["1", "s"], 
           RowBox[{
            RowBox[{"xyz", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "[", 
            RowBox[{"[", "1", "]"}], "]"}]}]}], ",", 
         RowBox[{"pQd2", "\[Rule]", 
          RowBox[{
           FractionBox["1", "s"], 
           RowBox[{
            RowBox[{"xyz", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "[", 
            RowBox[{"[", "2", "]"}], "]"}]}]}], ",", 
         RowBox[{"pQd3", "\[Rule]", 
          RowBox[{
           FractionBox["1", "s"], 
           RowBox[{
            RowBox[{"xyz", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "[", 
            RowBox[{"[", "3", "]"}], "]"}]}]}]}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"e1", ",", "e2", ",", "e3"}], "}"}], "=", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"eqPt1", ",", "eqPt2", ",", "eqPt3"}], "}"}], "/.", 
        "data1"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"data2", "=", 
       RowBox[{"{", 
        RowBox[{
